// ==UserScript==
// @name         Spotify Ad Blocker
// @namespace    https://www.spotify.com
// @version      1.0
// @description  Bypasses ads on Spotify Web Player - Made by King Trawal
// @author       King Trawal
// @match        https://*.spotify.com/*
// @grant        none
// ==/UserScript==

(function() {
    'use strict';

    // Replace the original Spotify ad function with an empty function if you want
    window.__defineGetter__('__spotify_ads', function() { return function() {}; });
})();
